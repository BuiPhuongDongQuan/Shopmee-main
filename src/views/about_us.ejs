        .box-container {
            display: flex;
            justify-content: center;
            align-items: center;
            flex-wrap: wrap;
            gap: 10px;
            height: 100vh;
        }
        .box {
            background: linear-gradient(to right, #56CCF2, #2F80ED);
            border: 2px solid #ccc;
            padding: 50px;
            box-sizing: border-box;
            font-family: Verdana, sans-serif;
            font-size: 18px;
            margin: 15px;
            text-align: center;
            cursor: pointer;
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            border-radius: 15px;
            width: 340px;
            height: 340px;
            box-shadow: 0px 0px 15px 0px rgba(0,0,0,0.75);
        }
        .modal-content {
            max-height: 70vh;
            overflow-y: auto;
        }
        .modal-image {
            width: 100%;
            height: auto;
        }
        .modal-body {
            padding: 1rem;
        }
        .square-frame {
            width: 240px;
            height: 240px;
            overflow: hidden;
        }
        .square-frame img {
            width: 100%;
            height: 100%;
            object-fit: cover;
        }
        /* Apply rounded corners to all images */
        img {
            border-radius: 15px;
        }
    </style>
